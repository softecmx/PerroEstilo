<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- tools:context=".ui.administrador.envios.detalleEnvios.DetalleEnvioFragment"-->
    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:elevation="5dp"
        app:cardCornerRadius="10dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="10dp"
                android:orientation="vertical">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/txtNombreDetEnvio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/txtPersonaRetira"
                        android:textColor="@color/black"
                        android:textSize="17dp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/valorNombrDetEnvio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/txtNombreDetEnvio"
                        android:text="@string/txtNombreContacto"
                        android:textColor="@color/black"
                        android:textSize="17dp"
                        android:textStyle="normal" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/txtNombreDireccionDetEnvio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/txtDireccionEntrega"
                        android:textColor="@color/black"
                        android:textSize="17dp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/valorDireccionDetEnvio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/txtNombreDireccionDetEnvio"
                        android:text="@string/txtMisDirecciones"
                        android:textColor="@color/black"
                        android:textSize="17dp"
                        android:textStyle="normal" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/txtCorreoDetEnvio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/correo"
                        android:textColor="@color/black"
                        android:textSize="17dp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/valorCorreoDetEnvio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/txtCorreoDetEnvio"
                        android:text="@string/txtCorreoUser"
                        android:textColor="@color/black"
                        android:textSize="17dp"
                        android:textStyle="normal" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/txtTelefonoDetEnvio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/txtTelefono"
                        android:textColor="@color/black"
                        android:textSize="17dp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/valorTelefonoDetEnvio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/txtTelefonoDetEnvio"
                        android:text="@string/telefono"
                        android:textColor="@color/black"
                        android:textSize="17dp"
                        android:textStyle="normal" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>



</LinearLayout>